marketingcookies.push(new makeCookieInfo('IDE','.doubleclick.net','','HTTP','DoubleClick tracking code via google adwords'));
marketingcookies.push(new makeCookieInfo('test_cookie','.doubleclick.net','Session','HTTP','Tests for cookie setting permissions for Doubleclick'));
marketingcookies.push(new makeCookieInfo('SID, NID, HSID, DV, SIDCC, SSID, APISID, SAPISID','.google.com','Max 1 year after session/update','HTTP','Google adwords tracking code'));
marketingcookies.push(new makeCookieInfo('rpb,put_,rpx,c,khaos','.rubiconproject.com','Max 1 year after session/update','HTTP','Rubiconproject tracking cookie via Google Adwords'));
marketingcookies.push(new makeCookieInfo('utm.gif','.google.com','Session','HTTP','Google Analytics pixel tracker'));
}
if(pardotcookies === true){
marketingcookies.push(new makeCookieInfo('pi_opt_in','.bynder.com','1 year','HTTP','This cookie registers if you consent with Pardot tracking'));
}
if(biziblecookies === true){
analyticalcookies.push(new makeCookieInfo('_biz_nA','.bynder.com','1 year','HTTP','This Bizible cookie tracks the number of pages you have seen'));
