	$(".sidenav a").each(function(){
		$(this).click(function(){
			console.log("click");
			setTimeout(function(){
			if($(".basiccookiecontainer").html()==""){
				returntable(".marketingcookiecontainer",marketingcookies);
				returntable(".basiccookiecontainer",neccessarycookies);
				returntable(".analyticalcookiecontainer",analyticalcookies);
			}
		 	}, 1500);
		});
