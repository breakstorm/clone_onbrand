var checkCompliance = function(){
	if(CCcheck('MARKETINGCONSENT') == '1'){
		$("#resourcesmenu").css("display","block").removeClass("ccboxafter");
		$(".cookieconsent-optout-marketing").css("display","none").removeClass("ccboxafter");
		$(".video-container-event").css("display","block").removeClass("ccboxafter");
		$(".carousel-inner .play").css("display","block").removeClass("ccboxafter");
		$("iframe[src*='youtube.com']").css("display","block").removeClass("ccboxafter");
		$("iframe[data-src*='youtube.com']").css("display","block").removeClass("ccboxafter");
		$("iframe[src*='vimeo']").css("display","block").removeClass("ccboxafter");
		$("iframe[data-src*='vimeo']").css("display","block").removeClass("ccboxafter");
		if(window.location.host == "developer-docs.bynder.com"){
			$("iframe[src*='vimeo']").css("display","block").removeClass("ccboxafter");
		}
		CCsetcookie("pi_opt_in","true",365);
		$("iframe[data-src*='youtube']").each(function(){
			var datasrc = $(this).attr("data-src");
			$(this).attr("src",datasrc);
		});
		$("iframe[data-src*='vimeo']").each(function(){
			var datasrc = $(this).attr("data-src");
			$(this).attr("src",datasrc);
		});
		$("script[data-cookieconsent='marketing']").each(function() {
			eval($(this).text());
		});

		$(".showcookies").on("click",function() {
			$("#cookieconsent").show();
			if($("#drift-widget-container").length >0){
				$("#drift-widget-container").css("z-index",1);
			}
		});

	}else{
		CCsetcookie("pi_opt_in","false",365);
		$("iframe[src*='youtube.com']").css("display","none");
		$("iframe[data-src*='youtube.com']").css("display","none");
		$("#resourcesmenu").css("display","none");
		$(".video-container-event").css("display","none");

		if(!$(".video-container-event").hasClass("ccboxafter")){
			$(".video-container-event").after('<div class="cookieconsent-optout-marketing padding-xl cookieconsent-border"><div><p>To view this embedded video content, you must accept marketing cookies.</p><p><a href="javascript:void(0)" class="btn btn-primary showcookies">Adjust your cookie preferences</a></p></div></div>');
		}
		if(!$(".carousel-inner .play").hasClass("ccboxafter")){
			$(".carousel-inner .play").after('<div class="cookieconsent-optout-marketing padding-xl"><div><p class="consentfont-s">To view this embedded video content, you must accept marketing cookies.</p><p><a href="javascript:void(0)" class="btn btn-inverse showcookies">Adjust your cookie preferences</a></p></div></div>');
		}

		$("#resourcesmenu").addClass("ccboxafter");
		$(".video-container-event").addClass("ccboxafter");
		$(".showcookies").on("click",function() {
			$("#cookieconsent").show();
			if($("#drift-widget-container").length >0){
				$("#drift-widget-container").css("z-index",1);
			}
		});

	}
}

checkCompliance();

