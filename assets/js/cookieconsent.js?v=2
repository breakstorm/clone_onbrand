analyticalcookies.push(new makeCookieInfo('_biz_uid','.bynder.com','1 year','HTTP','This Bizible cookie gives the visitor a user ID (anonymous)'));
}
if(analyticscookies === true){
analyticalcookies.push(new makeCookieInfo('_ga',currentsite,'2 years','HTTP','Google Analytics sets a cookie that is used to generate statistical data on how the visitor uses the website (anonymous).'));
analyticalcookies.push(new makeCookieInfo('_gid',currentsite,'1 day','HTTP','Registers a unique ID that is used to generate statistical data on how the visitor uses the website (anonymous).'));
analyticalcookies.push(new makeCookieInfo('_gat,_gat_gtag_UA*',currentsite,'Session','HTTP','Used by Google Analytics to throttle request rate'));
analyticalcookies.push(new makeCookieInfo('test_cookie',currentsite,'Session','HTTP','Tests for cookie setting permissions for Google Analytics'));
analyticalcookies.push(new makeCookieInfo('__utma',currentsite,'2 years','HTTP','Collects data on the number of times a user has visited the website as well as dates for the first and most recent visit. Used by Google Analytics. (anonymous)'));
analyticalcookies.push(new makeCookieInfo('__utmt',currentsite,'10 minutes','HTTP','Used to throttle the speed of requests to the server. (anonymous)'));
analyticalcookies.push(new makeCookieInfo('__utmb',currentsite,'30 minutes','HTTP','Registers a timestamp with the exact time of when the user accessed the website. Used by Google Analytics to calculate the duration of a website visit. (anonymous)'));
analyticalcookies.push(new makeCookieInfo('__utmz',currentsite,'30 minutes','HTTP','Collects data on where the user came from, what search engine was used, what link was clicked and what search term was used. Used by Google Analytics. (anonymous)'));
analyticalcookies.push(new makeCookieInfo('__utmx,__utmxx,__utmxx',currentsite,'1,5 years','HTTP','Google Optimize uses this cookie to determine a user\'s inclusion in an experiment. (Only when experiments are live)'));
analyticalcookies.push(new makeCookieInfo('HOMEPAGEVARIANT',currentsite,'1 year','HTTP','We use various names for A/B testing cookies. They all contain the word VARIANT'));
analyticalcookies.push(new makeCookieInfo('_hjIncludedInSample',currentsite,'Session','HTTP','We use HotJar for anonymous heatmaps so we can improve our navigation'));
}
if(websitecookies === true){
neccessarycookies.push(new makeCookieInfo('MARKETINGCONSENT',currentsite,'1 year','HTTP','We use this cookie to store your marketing cookie preference'));
neccessarycookies.push(new makeCookieInfo('cookieBar',currentsite,'2 years','HTTP','We use this cookie to remember if you closed the cookie notification bar (will be replaced 25th May 2018)'));
neccessarycookies.push(new makeCookieInfo('cfid, USERID, USERHASH',currentsite,'30 years','HTTP','We don\'t use this functional cookie, it\'s set by default by our CMS'));
neccessarycookies.push(new makeCookieInfo('MOBILEFORMAT, ORIGINALURLTOKEN',currentsite,'Session','HTTP','Is set to TRUE if you are using a mobile device'));
neccessarycookies.push(new makeCookieInfo('ORIGINALURLTOKEN',currentsite,'Session','HTTP','We don\'t use this functional cookie, it\'s set by default by our CMS'));
neccessarycookies.push(new makeCookieInfo('orbitAccountType',currentsite,'2 months','HTTP','The type of Orbit account you have (only set after a succesful Orbit login). Only used functionnaly.'));
neccessarycookies.push(new makeCookieInfo('orbitDomain',currentsite,'2 months','HTTP','The Orbit account domain you have (only set after a succesful Orbit login). Only used functionnaly.'));
neccessarycookies.push(new makeCookieInfo('__cfduid','*.bynder.com, .cloudflare.com, .hubspot.com, .hs-analytics.net, .hsforms.net, .hs-scripts.com','1 year','HTTP','Used by the content network, Cloudflare, to identify trusted web traffic.'));
}
if(driftcookies === true){
neccessarycookies.push(new makeCookieInfo('drift_aid',currentsite,'Session','HTTP','Drift chat identifier cookie (anonymous)'));
neccessarycookies.push(new makeCookieInfo('drift_sid',currentsite,'Session','HTTP','Drift chat identifier token for specific browser session (anonymous)'));
neccessarycookies.push(new makeCookieInfo('driftt_wmd',currentsite,'Session','HTTP','Drift chat boolean indicating the user has dismissed the welcome message in this session'));
neccessarycookies.push(new makeCookieInfo('DFTT_END_USER_ PREV_BOOTSTRAPPED',currentsite,'2 years','HTTP','Drift boolean indicating the user has previously been authenticated'));
}

var showconsentboxes=true;
var cookieinfoloaded=false;

function CCcheck(k){return(document.cookie.match('(^|; )'+k+'=([^;]*)')||0)[2]}
function CCsetcookie(e,t,i){var o=new Date;o.setTime(o.getTime()+24*i*60*60*1e3);var n="expires="+o.toUTCString();document.cookie=e+"="+t+";"+n+";domain=onbrand.me;path=/";}
function CCdeletecookie(name){document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT; domain=onbrand.me; path=/';}
function CCdeleteLegacycookie(name){document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT; domain=www.onbrand.me; path=/';}
function returntable(container,cookielist){
	for (index = 0; index < cookielist.length; ++index) {
    	$(container).append( '<tr><td>' + cookielist[index].name+ '</td><td>' + cookielist[index].provider + '</td><td>' + cookielist[index].description+ '</td><td>' + cookielist[index].duration+ '</td></tr>' );
    }
}

CCdeleteLegacycookie('MARKETINGCONSENT');

