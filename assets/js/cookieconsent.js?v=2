function makeCookieInfo(name,provider,duration,type,description){
	this.name = name;
	this.provider = provider;
	this.duration = duration;
	this.type = type;
	this.description = description;
}

var currentsite="onbrand.me";


var facebookcookies=true;
var linkedincookies=true;
var adwordscookies=true;
var analyticscookies=true;
var websitecookies=true;
var driftcookies=false;
var pardotcookies=true;
var biziblecookies=false;

var marketingcookies=Array();
var analyticalcookies=Array();
var neccessarycookies=Array();
if(facebookcookies === true){
	marketingcookies.push(new makeCookieInfo('Act, c_user, csm, fr, lu, s, presence, p, xs','.facebook.com','Max 1 year after session/update','HTTP','Used by Facebook to deliver a series of advertisement products such as real time bidding from third party advertisers. (internet based targeting)'));
}
if(linkedincookies === true){
marketingcookies.push(new makeCookieInfo('lidc, bcookie, bscookie','.linkedin.com','1 day','HTTP','LinkedIn Uses this cookie for routing data'));
marketingcookies.push(new makeCookieInfo('L1c','www.linkedin.com','Session','HTTP','LinkedIn browser ID cookie'));
marketingcookies.push(new makeCookieInfo('BizoID, BizoData, BizoUserMatchHistory, BizoNetworkPartnerIndex','www.linkedin.com','6 months','HTTP','LinkedIn Ad analytics'));
}
if(adwordscookies === true){
marketingcookies.push(new makeCookieInfo('collect','.google-analytics.com','Session','Pixel','Used to send data to Google Analytics about the visitor\'s device and behaviour. Tracks the visitor across devices and marketing channels.'));
marketingcookies.push(new makeCookieInfo('id','.yieldlab.com','','HTTP','Yieldlab ads tracking code via google adwords'));
